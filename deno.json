{
  "name": "@schematicos/codegen",
  "version": "0.0.1",
  "tasks": {
    "types": "deno check src/**/*.ts",
    "generate": "deno run --allow-read --allow-write --allow-env --allow-net src/cli/mod.ts"
  },
  "exports": {
    "./cli": "./src/cli/mod.ts"
  },
  "imports": {
    "@redocly/openapi-core": "npm:@redocly/openapi-core@1.11.0",
    "@schematicos/types": "npm:@schematicos/types@0.0.46",
    "cli": "jsr:@std/cli@0.222.1",
    "fs": "jsr:@std/fs@0.222.1",
    "assert": "jsr:@std/assert@0.222.1",
    "generate/": "./src/generate/",
    "lodash-es/": "https://esm.sh/lodash-es@4.17.21/",
    "mui-joy-forms/": "./src/transformers/mui-joy-forms/",
    "openapi-types":"npm:openapi-types@12.1.3",
    "parse/": "./src/parse/",
    "path": "jsr:@std/path@0.222.1",
    "prettier": "npm:prettier@3.2.5",
    "core/": "./src/core/",
    "rtk-query/": "./src/transformers/rtk-query/",
    "supabase-client/": "./src/transformers/supabase-client/",
    "tanstack-query/": "./src/transformers/tanstack-query/",
    "tiny-invariant": "npm:tiny-invariant@1.3.3",
    "ts-pattern": "npm:ts-pattern@5.1.1",
    "typescript/": "./src/languages/typescript/",
    "zod/": "./src/typeSystems/zod/"
  }
}

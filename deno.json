{
  "name": "@schematicos/codegen",
  "version": "0.0.13",
  "tasks": {
    "types": "deno check ./**/*.ts",
    "generate": "deno run --allow-read --allow-write --allow-env --allow-net cli/mod.ts generate",
    "init": "deno run --allow-read --allow-write --allow-env --allow-net cli/mod.ts init"
  },
  "exports": {
    "./cli": "./cli/mod.ts",
    "./CoreContext.ts": "./context/CoreContext.ts",
    "./Definition.ts": "./generate/elements/Definition.ts",
    "./generate/constants.ts": "./generate/constants.ts",
    "./generate/types.ts": "./generate/types.ts",
    "./naming.ts": "./generate/helpers/naming.ts",
    "./Operation.ts": "./parse/elements/Operation.ts",
    "./OperationSettings.ts": "./generate/settings/OperationSettings.ts",
    "./Parameter.ts": "./parse/elements/Parameter.ts",
    "./Ref.ts": "./parse/elements/Ref.ts",
    "./RequestBody.ts": "./parse/elements/RequestBody.ts",
    "./SchematicBase.ts": "./generate/elements/SchematicBase.ts",
    "./TransformerSettings.ts": "./generate/settings/TransformerSettings.ts",
    "./types": "./types/mod.ts",
    "./typescript/keyValues.ts": "./languages/typescript/helpers/keyValues.ts",
    "./typescript/toInferredType.ts": "./languages/typescript/toInferredType.ts",
    "./typescript/toOperationArg.ts": "./languages/typescript/helpers/toOperationArg.ts",
    "./typescript/toOperationResponse.ts": "./languages/typescript/helpers/toOperationResponse.ts",
    "./typescript/toParamsObject.ts": "./languages/typescript/helpers/toParamsObject.ts",
    "./typescript/toPathTemplate.ts": "./languages/typescript/helpers/toPathTemplate.ts",
    "./typescript/withDescription.ts": "./languages/typescript/helpers/withDescription.ts",
    "./zod": "./typeSystems/zod/mod.ts"
  },
  "imports": {
    "@babel/helper-validator-identifier": "npm:@babel/helper-validator-identifier@7.22.20",
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.4",
    "@cliffy/prompt": "jsr:@cliffy/prompt@1.0.0-rc.4",
    "@redocly/openapi-core": "npm:@redocly/openapi-core@1.11.0",
    "@schematicos/types": "npm:@schematicos/types@0.0.46",
    "assert": "jsr:@std/assert@0.223.0",
    "archive": "jsr:@std/archive@0.223.0",
    "context/": "./context/",
    "fs": "jsr:@std/fs@0.223.0",
    "generate/": "./generate/",
    "lodash-es": "npm:lodash-es@4.17.21",
    "mui-joy-forms/": "./transformers/mui-joy-forms/",
    "openapi-types": "npm:openapi-types@12.1.3",
    "parse/": "./parse/",
    "path": "jsr:@std/path@0.223.0",
    "prettier": "npm:prettier@3.2.5",
    "rtk-query/": "./transformers/rtk-query/",
    "supabase-client/": "./transformers/supabase-client/",
    "tanstack-query/": "./transformers/tanstack-query/",
    "tiny-invariant": "npm:tiny-invariant@1.3.3",
    "ts-pattern": "npm:ts-pattern@5.1.1",
    "types/": "./types/",
    "typescript/": "./languages/typescript/",
    "zod/": "./typeSystems/zod/",
    "zod": "npm:zod@3.23.4"
  },
  "publish": {
    "include": [
      "deno.json",
      "deno.lock",
      "LICENSE",
      "README.md",
      "cli/**/*",
      "context/**/*",
      "generate/**/*",
      "parse/**/*",
      "transformers/**/*",
      "core/**/*",
      "languages/**/*",
      "types/**/*",
      "typeSystems/**/*"
    ]
  }
}

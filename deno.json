{
  "name": "@schematicos/codegen",
  "version": "0.0.4",
  "tasks": {
    "types": "deno check src/**/*.ts",
    "generate": "deno run --allow-read --allow-write --allow-env --allow-net src/cli/mod.ts generate"
  },
  "exports": {
    "./cli": "./src/cli/mod.ts",
    "./mui-joy-forms": "./src/transformers/mui-joy-forms/mod.ts",
    "./rtk-query": "./src/transformers/rtk-query/mod.ts",
    "./supabase-client": "./src/transformers/supabase-client/mod.ts",
    "./tanstack-query": "./src/transformers/tanstack-query/mod.ts",
    "./zod": "./src/typeSystems/zod/mod.ts"
  },
  "imports": {
    "@babel/helper-validator-identifier": "npm:@babel/helper-validator-identifier@7.22.20",
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.4",
    "@redocly/openapi-core": "npm:@redocly/openapi-core@1.11.0",
    "@schematicos/types": "npm:@schematicos/types@0.0.46",
    "assert": "jsr:@std/assert@0.222.1",
    "core/": "./src/core/",
    "fs": "jsr:@std/fs@0.222.1",
    "generate/": "./src/generate/",
    "lodash-es": "npm:lodash-es@4.17.21",
    "mui-joy-forms/": "./src/transformers/mui-joy-forms/",
    "openapi-types": "npm:openapi-types@12.1.3",
    "parse/": "./src/parse/",
    "path": "jsr:@std/path@0.222.1",
    "prettier": "npm:prettier@3.2.5",
    "rtk-query/": "./src/transformers/rtk-query/",
    "supabase-client/": "./src/transformers/supabase-client/",
    "tanstack-query/": "./src/transformers/tanstack-query/",
    "tiny-invariant": "npm:tiny-invariant@1.3.3",
    "ts-pattern": "npm:ts-pattern@5.1.1",
    "typescript/": "./src/languages/typescript/",
    "zod/": "./src/typeSystems/zod/",
    "https://deno.land/": "./vendor/deno.land/"
  },
  "publish": {
    "include": [
      "deno.json",
      "deno.lock",
      "LICENSE",
      "README.md",
      "src/**/*.ts",
      "vendor/**/*"
    ]
  }
}
